{
  "manifest_version": 2,
  "name": "Tab list (clipboard & markdown)",
  "description": "Export the list of open tabs to the clipboard in the Markdown list.",
  "version": "1.0",
  "author": "Tomachi",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "List Tabs"
  },
  "permissions": [
    "tabs",
    "clipboardWrite",
    "notifications"
  ],
  "update_url": "https://github.com/book000/tablist/releases/latest/download/tablist.xpi"
}